<Window x:Name="mainWindow" x:Class="Director.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MiDeRP Director EPO2-D2" Height="600" Width="600" WindowStartupLocation="CenterScreen" SnapsToDevicePixels="True" MinHeight="400" MinWidth="400">
    <Grid>
        <Button x:Name="TestDrawButton" Content="Draw Field" Margin="0,0,90.6,10.4" HorizontalAlignment="Right" Width="75" Height="20" VerticalAlignment="Bottom" Click="TestDrawButton_Click"/>
        <Canvas x:Name="fieldmapcanvas" Margin="10,10,10.4,33.8" ClipToBounds="True"/>
        <Button x:Name="findPathButton" Content="Find Path" Margin="0,0,10.6,10.4" Click="findPathButton_Click" HorizontalAlignment="Right" Width="75" Height="20" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
